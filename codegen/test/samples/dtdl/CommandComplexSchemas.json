    "schemas": [
      {
        "@id": "dtmi:sharedSchemas:results;1",
        "@type": "Enum",
        "valueSchema": "string",
        "enumValues": [
          {
            "name": "success",
            "enumValue": "succès"
          },
          {
            "name": "failure",
            "enumValue": "échec"
          }
        ]
      },
      {
        "@id": "dtmi:sharedSchemas:someResponseInfo;1",
        "@type": "Object",
        "fields": [
          {
            "name": "overallResult",
            "schema": "dtmi:sharedSchemas:results;1"
          },
          {
            "name": "magnitude",
            "schema": "decimal"
          },
          {
            "name": "details",
