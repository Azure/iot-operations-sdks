{
  "metadata": [
    {
      "src": [{
        "src": "../../dotnet/src",
        "files": ["**/*.csproj"]
      }],
      "dest": "api",
      "filter": "filterConfig.yml",
      "namespaceLayout": "nested",
      "memberLayout": "samePage"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/*.yml"]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Azure IoT Operations .NET SDK",
      "_appTitle": "Azure IoT Operations .NET SDK",
      "_enableSearch": true,
      "pdf": false
    }
  }
}