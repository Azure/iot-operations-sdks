          {
            "name": "Blue",
            "enumValue": 3
          },
          {
            "name": "Yellow",
            "enumValue": 4
          },
          {
            "name": "Purple",
            "enumValue": 5
          },
          {
            "name": "Cyan",
            "enumValue": 6
          },
          {
            "name": "White",
            "enumValue": 7
          }
        ]
      },
