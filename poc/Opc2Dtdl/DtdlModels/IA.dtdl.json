    "@id": "dtmi:opcua:IA:IStatisticsType;1",
    "@type": [ "Interface", "Mqtt", "Congruence" ],
    "displayName": "IStatisticsType",
    "description": "Base interface for managing statistical data.",
    "extends": "dtmi:opcua:OpcUaCore:BaseInterfaceType;1",
    "typeRef": "nsu=http://opcfoundation.org/UA/IA;i=1011",
    "commandTopic": "opcua/IA/IStatisticsType/{commandName}",
    "telemetryTopic": "opcua/IA/IStatisticsType/{telemetryName}/{senderId}",
    "payloadFormat": "Json/ecma/404",
    "schemas": [
    ],
    "contents": [
      {
        "@type": [ "Property", "Qualified" ],
        "name": "StartTime",
        "displayName": "StartTime",
        "description": "Indicates the point in time at which the collection of the statistical data has been started.",
        "namespace": "http://opcfoundation.org/UA/IA",
        "schema": "dateTime",
        "writable": true
      },
      {
        "@type": [ "Command", "Qualified" ],
        "name": "ResetStatistics",
        "displayName": "ResetStatistics",
        "description": "Restarts all statistical data, including a reset of the StartTime to the current time.",
        "namespace": "http://opcfoundation.org/UA/IA"
      }
    ]
  },
  {
    "@context": [
