    "typeRef": "nsu=http://opcfoundation.org/UA/IA;i=1002",
    "commandTopic": "opcua/IA/BasicStacklightType/{commandName}",
    "telemetryTopic": "opcua/IA/BasicStacklightType/{telemetryName}/{senderId}",
    "payloadFormat": "Json/ecma/404",
    "schemas": [
      {
        "@id": "dtmi:opcua:IA:BasicStacklightType:dataType:StacklightOperationMode;1",
        "@type": [ "Enum", "Congruence" ],
        "displayName": "StacklightOperationMode",
        "description": "Contains the values used to indicate how a stacklight (as a whole unit) is used.",
        "typeRef": "nsu=http://opcfoundation.org/UA/IA;i=3002",
        "valueSchema": "integer",
        "enumValues": [
          {
            "name": "Segmented",
            "enumValue": 0
          },
          {
            "name": "Levelmeter",
            "enumValue": 1
          },
          {
            "name": "Running_Light",
            "enumValue": 2
          },
          {
            "name": "Other",
            "enumValue": 3
          }
        ]
      }
    ],
    "contents": [
      {
        "@type": [ "Property", "Qualified" ],
        "name": "StacklightMode",
        "displayName": "StacklightMode",
        "description": "Shows in what way (stack of individual lights, level meter, running light) the stacklight unit is used.",
        "namespace": "http://opcfoundation.org/UA/IA",
        "schema": "dtmi:opcua:IA:BasicStacklightType:dataType:StacklightOperationMode;1",
        "writable": true
      },
      {
        "@type": [ "Relationship", "Qualified" ],
        "maxMultiplicity": 1,
        "name": "StackLevel",
        "displayName": "StackLevel",
        "description": "Valid if the stacklight is used in “Levelmeter” StacklightMode. If so, the whole stack is controlled by a single percentual value. In this case, the SignalOn parameter of any stack element of StackElementLightType has no meaning.",
        "namespace": "http://opcfoundation.org/UA/IA",
        "target": "dtmi:opcua:IA:StackLevelType"
      },
      {
        "@type": [ "Relationship", "Qualified" ],
        "maxMultiplicity": 1,
        "name": "StackRunning",
        "displayName": "StackRunning",
