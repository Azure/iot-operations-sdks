    ],
    "@id": "dtmi:opcua:AMB:DocumentationLinksType;1",
    "@type": [ "Interface", "Article", "Mqtt", "Congruence" ],
    "ontology": "http://opcfoundation.org/UA/AMB",
    "displayName": "DocumentationLinksType",
    "description": "AddIn to link documentation provided by the manufacturer and / or end-user.",
    "extends": "dtmi:opcua:OpcUaCore:BaseObjectType;1",
    "typeRef": "nsu=http://opcfoundation.org/UA/AMB;i=1011",
    "commandTopic": "opcua/AMB/DocumentationLinksType/{commandName}",
    "telemetryTopic": "opcua/AMB/DocumentationLinksType/{telemetryName}/{senderId}",
    "payloadFormat": "Json/ecma/404",
    "schemas": [
    ],
    "contents": [
      {
        "@type": [ "Property", "Qualified" ],
        "name": "DefaultInstanceBrowseName",
        "displayName": "DefaultInstanceBrowseName",
        "namespace": "http://opcfoundation.org/UA/AMB",
        "schema": "string",
        "writable": true
      },
      {
        "@type": [ "Telemetry", "Qualified" ],
        "name": "Link",
