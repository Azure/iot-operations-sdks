    "@id": "dtmi:opcua:AMB:IRootCauseIndicationType;1",
    "@type": [ "Interface", "Mqtt", "Congruence" ],
    "displayName": "IRootCauseIndicationType",
    "description": "Information on the root cause of conditions, should be applied to alarms (AlarmType or subtypes)",
    "extends": "dtmi:opcua:OpcUaCore:BaseInterfaceType;1",
    "typeRef": "nsu=http://opcfoundation.org/UA/AMB;i=1002",
    "commandTopic": "opcua/AMB/IRootCauseIndicationType/{commandName}",
    "telemetryTopic": "opcua/AMB/IRootCauseIndicationType/{telemetryName}/{senderId}",
    "payloadFormat": "Json/ecma/404",
    "schemas": [
      {
        "@id": "dtmi:opcua:AMB:IRootCauseIndicationType:dataType:RootCauseDataType;1",
        "@type": [ "Object", "Congruence" ],
        "displayName": "RootCauseDataType",
        "description": "Root cause of an alarm",
        "typeRef": "nsu=http://opcfoundation.org/UA/AMB;i=3002",
        "fields": [
          {
            "@type": [ "Field", "Required" ],
            "name": "RootCauseId",
            "schema": "string"
          },
          {
            "@type": [ "Field", "Required" ],
            "name": "RootCause",
            "schema": "string"
          }
        ]
      }
    ],
    "contents": [
      {
        "@type": [ "Property", "Qualified" ],
        "name": "PotentialRootCauses",
        "displayName": "PotentialRootCauses",
        "description": "An array of potential root causes of the alarm. This is intended to be a hint to the client and might be a local view on the potential root causes of the alarm. The list might not contain all potential root causes, that is, other potential root causes might exist as well. If the alarm itself is considered to be the root cause, the array shall be empty. If no potential root causes have been identified, there shall be at least one entry in the array indicating that the root cause is unknown.",
        "namespace": "http://opcfoundation.org/UA/AMB",
        "schema": {
          "@type": "Array",
          "elementSchema": "dtmi:opcua:AMB:IRootCauseIndicationType:dataType:RootCauseDataType;1"
        },
        "writable": true
      }
    ]
  },
  {
    "@context": [
