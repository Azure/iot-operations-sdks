"<#=this.eventName#>": {
<# if (this.uaVariable.BrowseNamespace != null) { #>
  "aov:namespace": "<#=this.uaVariable.BrowseNamespace#>",
<# } #>
  "data": {
<# this.PushIndent("    "); #>
<#=this.dataSchema.TransformText()#>
<# this.PopIndent(); #>
  },
<# if (this.uaVariable.IsPlaceholder) { #>
  "dtv:placeholder": true,
<# } #>
<# if (this.containedIn != null) { #>
  "aov:containedIn": "<#=this.containedIn#>",
<# } #>
<# if (this.contains.Any()) { #>
  "aov:contains": [ <#=string.Join(", ", this.contains.Select(c => $"\"{c}\""))#> ],
<# } #>
  "forms": [
    {
      "contentType": "application/json",
      "dtv:topic": "opcua/<#=this.specName#>/<#=this.thingModelName#>/event/<#=this.eventName#>/{senderId}",
      "op": "subscribeevent"
    }
  ]
}