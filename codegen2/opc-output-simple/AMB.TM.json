    "properties": {
      "PotentialRootCauses": {
        "aov:namespace": "http://opcfoundation.org/UA/AMB/",
        "description": "An array of potential root causes of the alarm. This is intended to be a hint to the client and might be a local view on the potential root causes of the alarm. The list might not contain all potential root causes, that is, other potential root causes might exist as well. If the alarm itself is considered to be the root cause, the array shall be empty. If no potential root causes have been identified, there shall be at least one entry in the array indicating that the root cause is unknown.",
        "type": "array",
        "items": {
          "title": "RootCauseDataType",
          "description": "Root cause of an alarm",
          "type": "object",
          "aov:typeRef": "nsu=http://opcfoundation.org/UA/AMB/;i=3002",
          "required": [ "RootCauseId", "RootCause" ],
          "properties": {
            "RootCauseId": {
              "description": "The NodeId of the root cause of an alarm. This can point to another Node in the AddressSpace or a ConditionId, that is not necessarily represent as Object in the AddressSpace. Ideally, this points directly to the root cause. Potentially, it points to an alarm that has an additional root cause. Clients shall expect, that they need to follow a path to find the root cause. If the root cause is unknown, the NodeId shall be set to NULL.",
              "type": "string"
            },
            "RootCause": {
              "description": "Localized description of the root cause of an alarm. This can be the DisplayName of the Node referenced by RootCauseId or a more descriptive text. If the root cause is unknown, this should be described in the text.",
              "type": "string"
            }
          }
        },
        "readOnly": false,
        "forms": [
          {
            "contentType": "application/json",
            "dtv:topic": "opcua/AMB/AMB_IRootCauseIndicationType/property/PotentialRootCauses/write",
            "op": "writeproperty"
          },
          {
            "contentType": "application/json",
            "dtv:topic": "opcua/AMB/AMB_IRootCauseIndicationType/property/PotentialRootCauses/read",
            "op": "readproperty"
          }
        ]
      }
    },
