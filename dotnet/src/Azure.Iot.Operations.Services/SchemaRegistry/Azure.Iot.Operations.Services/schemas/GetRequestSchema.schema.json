{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "GetRequestSchema",
  "description": "GET Schema request object",
  "type": "object",
  "additionalProperties": false,
  "required": [ "name", "version" ],
  "properties": {
    "name": {
      "description": "Schema name.",
