{
    "@context": [
      "dtmi:dtdl:context;3",
      "dtmi:dtdl:extension:quantitativeTypes;1",
      "dtmi:dtdl:extension:mqtt;1"
    ],
    "@id": "dtmi:akri:samples:oven;1",
    "@type": [ "Interface", "Mqtt" ],
    "telemetryTopic": "akri/samples/{modelId}/{senderId}",
    "payloadFormat": "Json/ecma/404",
    "contents": [
      {
        "@type": [
          "Telemetry",
          "Temperature"
        ],
        "name": "internalTemperature",
        "schema": "double",
        "unit": "degreeCelsius"
      },
      {
        "@type": [
          "Telemetry",
          "Temperature"
        ],
        "name": "externalTemperature",
        "schema": "double",
        "unit": "degreeCelsius"
      },
      {
        "@type": "Telemetry",
        "name": "operationSummary",
        "schema": {
            "@type": "Object",
            "fields": [
                {
                    "name": "startingTime",
                    "schema": "dateTime"
                },
                {
                    "name": "totalDuration",
                    "schema": "duration"
                },
                {
                    "name" : "numberOfCakes",
                    "schema": "long"                    
                }
            ]
        }
      }
    ]
}