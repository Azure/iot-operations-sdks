{
  "deviceTypeId": "DeluxeDevice",
  "modelId": "dtmi:SemanticDataModel:FirstModel;1",
  "Telemetry": {
    "ThermalCondition": {
      "InternalTemp": [ "@quant", "$[*].Measurement.Equipment.Internal.Temperature", "Temperature(degreeCelsius)" ],
      "ExternalTemp": [ "@quant", "$[*].Measurement.Equipment.Surface.Temperature", "Temperature(degreeCelsius)" ]
    },
    "ArmPosition": [
      "$[*].Control.Position.x",
      "$[*].Control.Position.y"
    ],
    "Status": [
      "@map",
      "$[*].Measurement.Equipment.Status",
      {
        "Good": 0,
        "Warn": 1,
        "Alert": 2,
        "Panic": 2
      }
    ],
    "Mode": "$[*].Control.Mode"
  }
}
